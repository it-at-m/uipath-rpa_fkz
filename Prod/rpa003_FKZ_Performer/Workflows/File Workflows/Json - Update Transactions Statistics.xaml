    <ui:InvokeCode ContinueOnError="{x:Null}" Code="Dim dict_refTxns As New List(Of Dictionary(Of String, Object))&#xD;&#xA;&#xD;&#xA;For Each kvp In in_dict_txns&#xD;&#xA;    Dim val = kvp.Value&#xD;&#xA;    If TypeOf val Is JObject Then&#xD;&#xA;        dict_refTxns.Add(CType(val, JObject).ToObject(Of Dictionary(Of String, Object))())&#xD;&#xA;    ElseIf TypeOf val Is Dictionary(Of String, Object) Then&#xD;&#xA;        dict_refTxns.Add(DirectCast(val, Dictionary(Of String, Object)))&#xD;&#xA;    End If&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;Dim int_countSkipped, int_countToProcess, int_countSuccess, int_countBE, int_countSE, int_countSendReminder As Integer&#xD;&#xA;Dim ts_totalDuration As New TimeSpan(0)&#xD;&#xA;&#xD;&#xA;For Each dict_txnsItem As Dictionary(Of String, Object) In dict_refTxns&#xD;&#xA;    ' Status holen&#xD;&#xA;    Dim str_status As String = Nothing&#xD;&#xA;    If dict_txnsItem.ContainsKey(&quot;Status&quot;) AndAlso dict_txnsItem(&quot;Status&quot;) IsNot Nothing Then&#xD;&#xA;        str_status = dict_txnsItem(&quot;Status&quot;).ToString&#xD;&#xA;    End If&#xD;&#xA;&#x9;&#xD;&#xA;    Select Case str_status&#xD;&#xA;        Case &quot;Neu&quot;, &quot;Nicht Hochgeladen&quot;&#xD;&#xA;            int_countSkipped += 1&#xD;&#xA;        Case &quot;Hochgeladen&quot;&#xD;&#xA;            int_countToProcess += 1&#xD;&#xA;        Case &quot;Erfolgreich&quot;&#xD;&#xA;            int_countSuccess += 1&#xD;&#xA;            If dict_txnsItem.ContainsKey(&quot;Bemerkung&quot;) AndAlso dict_txnsItem(&quot;Bemerkung&quot;) IsNot Nothing Then&#xD;&#xA;                Dim str_bemerkung As String = dict_txnsItem(&quot;Bemerkung&quot;).ToString()&#xD;&#xA;                If str_bemerkung.Contains(&quot;Reminder versendet.&quot;) Then&#xD;&#xA;                    int_countSendReminder += 1&#xD;&#xA;                End If&#xD;&#xA;            End If&#xD;&#xA;        Case &quot;Ausgesteuert&quot;&#xD;&#xA;            int_countBE += 1&#xD;&#xA;        Case &quot;Systemfehler&quot;&#xD;&#xA;            int_countSE += 1&#xD;&#xA;    End Select&#xD;&#xA;&#x9;&#xD;&#xA;    If in_bool_shouldSumDuration AndAlso dict_txnsItem.ContainsKey(&quot;Dauer&quot;) AndAlso dict_txnsItem(&quot;Dauer&quot;) IsNot Nothing Then&#xD;&#xA;        Dim str_txnsItemDuration As String = dict_txnsItem(&quot;Dauer&quot;).ToString.Trim&#xD;&#xA;        If str_txnsItemDuration &lt;&gt; &quot;&quot; Then&#xD;&#xA;            Dim ts As TimeSpan&#xD;&#xA;            If TimeSpan.TryParse(str_txnsItemDuration, ts) Then&#xD;&#xA;                ts_totalDuration = ts_totalDuration.Add(ts)&#xD;&#xA;            End If&#xD;&#xA;        End If&#xD;&#xA;    End If&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;io_Config(&quot;CountTotal&quot;) = dict_refTxns.Count&#xD;&#xA;io_Config(&quot;CountSkipped&quot;) = int_countSkipped&#xD;&#xA;io_Config(&quot;CountToProcess&quot;) = int_countToProcess&#xD;&#xA;io_Config(&quot;CountSuccess&quot;) = int_countSuccess&#xD;&#xA;io_Config(&quot;CountBE&quot;) = int_countBE&#xD;&#xA;io_Config(&quot;CountSE&quot;) = int_countSE&#xD;&#xA;io_Config(&quot;CountSendReminder&quot;) = int_countSendReminder&#xD;&#xA;io_Config(&quot;SumDuration&quot;) = ts_totalDuration.ToString(&quot;hh\:mm\:ss&quot;)" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="416,188" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
